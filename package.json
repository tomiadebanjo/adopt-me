{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "A Pet shop",
  "main": "index.js",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "start": "npm run build && babel-node server.js",
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@reach/router": "^1.2.1",
    "babel-cli": "^6.26.0",
    "emotion": "^10.0.9",
    "express": "^4.17.0",
    "petfinder-client": "^1.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^10.0.0"
  }
}
